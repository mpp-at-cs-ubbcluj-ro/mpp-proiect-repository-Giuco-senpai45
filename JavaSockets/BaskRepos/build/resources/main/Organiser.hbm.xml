<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bask.model">
    <class name="Organiser"
           table="Organiser">
        <id name="id" column="Id">
            <generator class="increment"/>
        </id>
        <property
                name="name"
                column="Name"/>
        <property
                name="password"
                column="Password"/>
    </class>
</hibernate-mapping>